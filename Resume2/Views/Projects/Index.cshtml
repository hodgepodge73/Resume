@model IEnumerable<Resume2.Models.Project>

@{
    ViewData["Title"] = "Projects";
}

<h1>Project List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                Url
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.url)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.title">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.title">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.title">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
